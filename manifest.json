{
    "manifest_version":1,
    "type":"WebApp",
    "meta":{
        "name":"OverPresence",
        "version":"1.0.0",
        "minimum-overwolf-version":"0.77.10",
        "author":"DirectdDev",
        "icon":"icon.png",
        "icon_gray":"IconMouseNormal.png",
        "launcher_icon": "desktop-icon.ico",
        "description":"Discord rich presence for you overwolf and games"
    },
    "data": {
        "start_window":"MainWindow",
        "windows":{
            "MainWindow":{
                    "file":"index.html",
                    "transparent": false,
                    "resizable": true,
                    "use_os_windowing": true,
                    "size": {
                        "width":700,
                        "height":400
                        },
                    "min_size": {
                        "width":400,
                        "height":400
                    }
                }
        }
    }
}
 "data": {
